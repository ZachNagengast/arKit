{    
    "page_background": {
      "backgroundColor": "#ccc"  
    },
    
    "title_label": {
        "textAlignment": "center",
        "font": "title_font",
        "textColor": "#666",
        "shadowColor": "#eb743b",
        "shadowOffset": { "width": 1, "height": 1 }
    },
    
    "description_label": {
        "font": "body_font",
        "textColor": "#333",
        "backgroundColor": "#fff",
        "numberOfLines": 99
    },
    
    "avatar_style": {
        "image": "default_user_pic",
        "clipsToBounds": true,
        "layer": {
            "borderColor": "#ac0000",
            "borderWidth": 1,
            "cornerRadius": 6
        }
    },
    
    "a_view_style": {
        "backgroundColor": "pattern"
    },
    
    "b_view_style": {
        "backgroundColor": { "color": "#ac0000" }
    },
    
    "button_style1": {
        "backgroundImage": { "color": "#333", "radius": 8 },
        "titleColor": "#fff",
        "layer": {
            "borderColor": "#fff",
            "borderWidth": 3,
            "shadowColor": "#eb743b",
            "shadowOpacity": 0.8,
            "shadowRadius": 2,
            "shadowOffset": { "width": 2, "height": 2 },
            "cornerRadius": 6
        },
        "showsTouchWhenHighlighted": false
    },
    
    "button_style2": {
        "backgroundImage": { "color": "#333", "radius": 8 },
        "titleColor": "#fff",
        "layer": {
            "borderColor": "#fff",
            "borderWidth": 3,
            "shadowColor": "#eb743b",
            "shadowOpacity": 0.8,
            "shadowRadius": 2,
            "shadowOffset": { "width": 2, "height": 2 },
            "cornerRadius": 6
        },
        "showsTouchWhenHighlighted": true
    },
}
